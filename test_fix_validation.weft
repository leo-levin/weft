// Test to validate the StrandRemap fixes
// This should now work properly with coordinate swapping

// Create a simple pattern that's clearly asymmetric
let pattern = me@x * me@y

// Create the original pattern
pattern :: original<val>

// Create a coordinate-swapped version
original@val(me@y, me@x) :: swapped<val>

// Display both - should see the effect of coordinate swapping
display(original@val, swapped@val, abs(original@val - swapped@val))